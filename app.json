{
  "expo": {
    "name": "Moin",
    "slug": "Moin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobileProgramming.moin",
      "googleServicesFile": "./setting/GoogleService-Info.plist",
      "config": {
        "googleSignIn": {
          "reservedClientId": "932679524918-uhu51egt7aigkb1arqcn9eudtvrdtgfn.apps.googleusercontent.com" 
        }
      }
    },
    "android": {
      "package": "com.mobileProgramming.moin",
      "googleServicesFile": "./setting/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode" : "resize"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
